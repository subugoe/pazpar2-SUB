<settings target="z3950.libris.kb.se/*">
<!--
	For of the catalogues of Sweden’s national library.

	2012: Sven-S. Porst, SUB Göttingen <porst@sub.uni-goettingen.de>
-->

	<!-- mappings for search -->
	<set name="pz:cclmap:term" value="u=1016 t=l,r s=al"/>

	<set name="pz:cclmap:title" value="u=4 s=al 2=102"/>
	<set name="pz:cclmap:person" value="u=1003 s=al"/>

	<set name="pz:cclmap:subject" value="u=21 s=al"/>
	<set name="pz:cclmap:isbn" value="u=7"/>
	<set name="pz:cclmap:issn" value="u=8"/>


	<!-- Retrieval settings -->
	<set name="pz:requestsyntax" value="marc21"/>
	<set name="pz:elements" value="F"/>
	<set name="pz:queryencoding" value="utf-8"/>

	<!-- Server does not support piggybacking -->
	<set name="pz:piggyback" value="0"/>


	<!-- Result normalization settings -->
	<set name="pz:nativesyntax" value="iso2709;utf-8"/>
	<set name="pz:xslt" value="xsl/MarcXML2TurboMarc.xsl,pazpar2-etc/tmarc.xsl,xsl/medium-strip-electronic.xsl,xsl/catalogue-url-hint+id.xsl,xsl/merge-fields.xsl"/>

	<set name="catalogueURLHintPrefix" target="z3950.libris.kb.se/*" value="http://libris.kb.se/bib/"/>

</settings>

